<html>
	<head>
		<title>Untitled</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
		<% require themedCSS('assets/css/main.css') %>
		<% require themedCSS('assets/css/noscript.css') %>
	
	</head>
	<body class="landing is-preload">

		<!-- Page Wrapper -->
			<div id="page-wrapper">

				<!-- Header -->
					<header id="header" class="alt">
						<h1><a href="index.html">Spectral</a></h1>
						<nav id="nav">
							<ul>
								<li class="special">
									<a href="#menu" class="menuToggle"><span>Menu</span></a>
									<div id="menu">
										<ul>
											<li><a href="index.html">Home</a></li>
											<li><a href="generic.ss">Generic</a></li>
											<li><a href="elements.html">Elements</a></li>
											<li><a href="#">Sign Up</a></li>
											<li><a href="#">Log In</a></li>
										</ul>
									</div>
								</li>
							</ul>
						</nav>
					</header>

				<!-- Banner -->
					<section id="banner">
						<div class="inner">
							<h2>$Title</h2>
							<p>$Subtitle</p>
							<ul class="actions special">
								<li><a href="#" class="button primary">Activate</a></li>
							</ul>
						</div>
						<a href="#one" class="more scrolly">Learn More</a>
					</section>

				<!-- One -->
				<section id="one" class="wrapper style1 special">
						<div class="inner">
							<header class="major">
								<h2>$IconTitle</h2>
								<p>$IconContent</p>
							</header>
							<ul class="icons major">
								<li><span class="icon $IconOne major style1"><span class="label">Lorem</span></span></li>
								<li><span class="icon $IconTwo major style2"><span class="label">Ipsum</span></span></li>
								<li><span class="icon $IconThree major style3"><span class="label">Dolor</span></span></li>
							</ul>
						</div>
					</section>
				<!-- Two -->
					<section id="two" class="wrapper alt style2">
						<section class="spotlight">
							<div class="image">$ContentImageOne</div><div class="content">
								<h2>$ImageTitleOne</h2>
								<p>$ImageParagrahOne</p>
							</div>
						</section>
						<section class="spotlight">
							<div class="image"><img src="/_resources/themes/spectral/images/pic02.jpg" alt="" /></div><div class="content">
								<h2>$ImageTitleTwo</h2>
								<p>$ImageParagrahTwo</p>
							</div>
						</section>
						<section class="spotlight">
							<div class="image"><img src="/_resources/themes/spectral/images/pic03.jpg" alt="" /></div><div class="content">
								<h2>$ImageTitleThree</h2>
								<p>$ImageParagrahThree</p>
							</div>
						</section>
					</section>

				<!-- Three -->
					<section id="three" class="wrapper style3 special">
						<div class="inner">
							<header class="major">
								<h2>$FeatureTitle</h2>
								<p>$FeatureSubtitle</p>
							</header>
							<ul class="features">
								<li class="icon fa-paper-plane">
									<h3>$FeatureOneTitle</h3>
									<p>$FeatureOneContent</p>
								</li>
								<li class="icon solid fa-laptop">
									<h3>$FeatureTwoTitle</h3>
									<p>$FeatureTwoContent</p>
								</li>
								<li class="icon solid fa-code">
									<h3>$FeatureThreeTitle</h3>
									<p>$FeatureThreeContent</p>
								</li>
								<li class="icon solid fa-headphones-alt">
									<h3>$FeatureFourTitle</h3>
									<p>$FeatureFourContent</p>
								</li>
								<li class="icon fa-heart">
									<h3>$FeatureFiveTitle</h3>
									<p>$FeatureFiveContent</p>
								</li>
								<li class="icon fa-flag">
									<h3>$FeatureSixTitle</h3>
									<p>$FeatureSixContent</p>
								</li>
							</ul>
						</div>
					</section>

				<!-- CTA -->
					<section id="cta" class="wrapper style4">
						<div class="inner">
							<header>
								<h2>$FooterTitle</h2>
								<p>$FooterContent</p>
							</header>
							<ul class="actions stacked">
								<li><a href="#" class="button fit primary">Activate</a></li>
								<li><a href="#" class="button fit">Learn More</a></li>
							</ul>
						</div>
					</section>

				<!-- Footer -->
					<footer id="footer">
						<ul class="icons">
							<li><a href="#" class="icon brands fa-twitter"><span class="label">Twitter</span></a></li>
							<li><a href="#" class="icon brands fa-facebook-f"><span class="label">Facebook</span></a></li>
							<li><a href="#" class="icon brands fa-instagram"><span class="label">Instagram</span></a></li>
							<li><a href="#" class="icon brands fa-dribbble"><span class="label">Dribbble</span></a></li>
							<li><a href="#" class="icon solid fa-envelope"><span class="label">Email</span></a></li>
						</ul>
						<ul class="copyright">
							<li>&copy; Untitled</li>
						</ul>
					</footer>

			</div>


$Form
		<!-- Scripts -->
		<% require themedJavascript('jquery.min.js') %>
		<% require themedJavascript('jquery.scrollex.min.js') %>
		<% require themedJavascript('jquery.scrolly.min.js') %>
		<% require themedJavascript('browser.min.js') %>
		<% require themedJavascript('breakpoints.min.js') %>
		<% require themedJavascript('util.js') %>
		<% require themedJavascript('main.js') %>	
		

	</body>
</html>